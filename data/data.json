    "key": "payment_confirmation",
    "title": "KES{amount} {product} for {number} will be deducted from {payment_method_text}:",
    "options": {
      "1": {
        "label": "Accept",
        "value": "1",
        "next": "payment_end"
      },
      "2": {
        "label": "Cancel",
        "value": "2",
        "next": "cancel"
      },
      "3": {
        "label": "Other MPESA number",
        "value": "3",
        "next": "payment_other_number_mpesa"
      }
