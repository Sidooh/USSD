  "refer_pin": {
    "key": "refer_pin",
    "title": "Please enter your Sidooh PIN",
    "type": "OPEN",
    "next": "refer",
    "option_type": "PIN"
  },
  "refer": {
    "key": "refer",
    "title": "Invite your friends to Sidooh and start earning from every airtime they purchase from the platform once they enroll within {enrollment_time}hrs. Enter no:",
    "type": "OPEN",
    "next": "refer_end",
    "option_type": "PHONE"
  },
  "refer_end": {
    "key": "refer_end",
    "title": "Thank you for referring Sidooh. Your friend will receive an invite SMS. Once they register and buy airtime within {enrollment_time}hrs, you will start earning from every airtime they purchase from Sidooh.",
    "type": "OPEN"
  },
  "payment_end": {
    "key": "payment_end",
    "title": "Your request has been received and is being processed. {payment_method_instruction}. Thank you.",
    "type": "END"
  },
  "coming_soon": {
    "key": "coming_soon",
    "title": "COMING SOON. We shall inform you once the service is available.",
    "type": "OPEN"
  },
