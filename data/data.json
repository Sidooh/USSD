    "type": "OPEN",
    "next": "payment_confirmation_final",
    "validations": "SAFARICOM,DISALLOW_CURRENT"
  },
  "payment_confirmation_final": {
    "key": "payment_confirmation_final",
    "title": "KES{amount} {product} for {number} will be deducted from {payment_method_text}:",
    "options": {
      "1": {
        "label": "Accept",
        "value": "1",
        "next": "payment_end"
      },
      "2": {
        "label": "Cancel",
        "value": "2",
        "next": "cancel"
      }
    }
  },
